%%%%%%%%%%%%%% _BOXES_ %%%%%%%%%%%%%%%%%
\newtcolorbox{chapbox}[2][]{
enforce breakable,
enhanced jigsaw,
colback=white,
after skip=1mm,
boxrule=1pt,
toprule=1.5pt,
rightrule=-1pt,
frame style={left color= iorange!90!Fire1, right color= white},
attach boxed title to top left ={
    xshift=1cm,yshift*=1mm-\tcboxedtitleheight},
    varwidth boxed title*=-3cm,
    boxed title style={
        frame code={
            \path[fill=black!30!black] 
            ([yshift=-1mm,xshift=-1mm]frame.north west)
            arc[start angle=0,end angle=180,radius=1mm]
            ([yshift=-1mm,xshift=1mm]frame.north east)
            arc[start angle=180,end angle=0,radius=1mm];
            \path[bottom color=diorange!90!Fire1, middle color=iorange, top color=liorange!90!white]
            ([xshift=-2mm]frame.north west) -- ([xshift=2mm]frame.north east)
            [rounded corners=1mm]-- ([xshift=1mm,yshift=-1mm]frame.north east)
            -- (frame.south east) -- (frame.south west)
            -- ([xshift=-1mm,yshift=-1mm]frame.north west)
            [sharp corners]-- cycle;
            },interior engine=empty,
    },
fonttitle=\bfseries\Large,
title={#2},
enforce breakable,
pad at break*=0mm,
}

\newtcolorbox[auto counter,number within=section]{secbox}[2][]{
enforce breakable,
enhanced jigsaw,
before skip=5mm,after skip=2mm,
colback=white,
boxrule=1pt,
rightrule=-0.5pt,
frame style={right color = white, left color=diblue},
attach boxed title to top left={
    xshift=1cm,yshift*=1mm-\tcboxedtitleheight},
    varwidth boxed title*=-3cm,
    boxed title style={
        frame code={
            \path[fill=black!30!black] 
            ([yshift=-1mm,xshift=-1mm]frame.north west)
            arc[start angle=0,end angle=180,radius=1mm]
            ([yshift=-1mm,xshift=1mm]frame.north east)
            arc[start angle=180,end angle=0,radius=1mm];
            \path[bottom color=diblue, middle color=ipink, top color=liblue,]
            ([xshift=-2mm]frame.north west) -- ([xshift=2mm]frame.north east)
            [rounded corners=1mm]-- ([xshift=1mm,yshift=-1mm]frame.north east)
            -- (frame.south east) -- (frame.south west)
            -- ([xshift=-1mm,yshift=-1mm]frame.north west)
            [sharp corners]-- cycle;
            },interior engine=empty,
        },
fonttitle=\bfseries\large,
title=\thetcbcounter~{#2},
enforce breakable,
pad at break*=0mm,
}

\newtcolorbox{probbox}[2][]{
enhanced,
before skip=5mm,after skip=2mm,
colback=white,
boxrule=-1pt,
toprule=1pt,
frame style={right color = white, left color=dipink},
attach boxed title to top left={
    xshift=1cm,yshift*=1mm-\tcboxedtitleheight},
    varwidth boxed title*=-3cm,
    boxed title style={
        frame code={
            \path[fill=black!30!black] 
            ([yshift=-1mm,xshift=-1mm]frame.north west)
            arc[start angle=0,end angle=180,radius=1mm]
            ([yshift=-1mm,xshift=1mm]frame.north east)
            arc[start angle=180,end angle=0,radius=1mm];
            \path[bottom color=dipink, middle color=ipink, top color=lipink,]
            ([xshift=-2mm]frame.north west) -- ([xshift=2mm]frame.north east)
            [rounded corners=1mm]-- ([xshift=1mm,yshift=-1mm]frame.north east)
            -- (frame.south east) -- (frame.south west)
            -- ([xshift=-1mm,yshift=-1mm]frame.north west)
            [sharp corners]-- cycle;
            },interior engine=empty,
    },
fonttitle=\bfseries\small,
title={#2},#1,
}

\newtcolorbox{defbox}{
enhanced,
interior style ={bottom color=Moon1!10!white, top color=Moon2!5!white},
colframe=white,
sharp corners,
boxrule=1mm,
  underlay={\begin{tcbclipframe}\tcbvignette{
    size=1mm,
    draw method=clipped,
    north style={black!80,path fading=south},
    east style={black!30,path fading=west},
    south style={black!10,path fading=north},
    west style={black!60,path fading=east},
    inside node=frame
  }\end{tcbclipframe}}
}

\newtcbox{look}{
